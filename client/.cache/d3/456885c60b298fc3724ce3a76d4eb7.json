assName=\"main-view\">\r\n        {selectedMovie ? (\r\n          <MovieView movie={selectedMovie} goBack={this.onButtonClick} />\r\n        ) : (\r\n          movies.map(movie => (\r\n            <MovieCard\r\n              key={movie._id}\r\n              movie={movie}\r\n              onMovieClick={movie => this.onMovieClick(movie)}\r\n            />\r\n          ))\r\n        )}\r\n      </div>\r\n    );\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f090a10c75f493020716e37660226e15","cacheData":{"env":{}}}